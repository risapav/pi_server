{
  "plugins": ["html"],
  "env": {
    "jquery": true,
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "impliedStrict": true
  },
  "rules": {
    "no-console": "off"
  }
}
